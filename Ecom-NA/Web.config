<configuration>
	<connectionStrings>
		<add name="EcomNADB" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=EcomNADB;Integrated Security=True" providerName="System.Data.sqlClient"/>
		
		
	</connectionStrings>
<!---This appsettings is for the signin page where ASP.NET WebForms 4.5+, the validators (like RequiredFieldValidator, RegularExpressionValidator) rely on jQuery if UnobtrusiveValidationMode is enabled. so it give error due to jquery. We will disable Unobtrusive validation to make our own validations to work for the textbox and all.-->
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
		<add key="PageInspector:ServerCodeMapingSupport" value="Disabled" />
	</appSettings>
	
	
  <system.web>
